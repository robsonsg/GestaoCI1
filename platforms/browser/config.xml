<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.gestaoci" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>GestaoCI</name>
    <description>
        Gestao Cidades Inteligentes
    </description>
    <author email="robsonsg@terra.com.br" href="http://dese.gestaoci.com.br">
        Robson Santos Gomes
    </author>
    <content src="/Views/Home/Index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
